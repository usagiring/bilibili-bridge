    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@types/koa__router": "^8.0.4",
    "@types/mocha": "^8.2.3",
    "@types/nedb": "^1.8.11",
    "@types/node": "^15.0.3",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
