{
  "name": "@tokine/bilibili-bridge",
  "version": "0.1.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:asr && npm run build:mt && tsc",
    "start": "node ./dist/index",
    "start:test": "node ./bin/run-test.js",
    "dev": "nodemon --watch 'lib/**' --ext 'ts,json' --exec 'ts-node index.ts test'",
    "dev:win": "nodemon --watch \"lib/**\" --ext \"ts,json\" --exec \"ts-node index.ts test\"",
    "test": "mocha",
    "prepublish": "npm run build",
    "lint": "eslint lib/**/*",
    "release": "rm -rf dist && npm run build && npm publish",
    "link": "rm -rf dist && npm run build && npm link",
    "build:asr": "cd asr && npm run build",
    "build:mt": "cd mt && npm run build"
  },
  "author": "其妙",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@node-rs/jieba": "^1.4.1",
    "@tokine/asr": "file:./asr",
    "@tokine/mt": "file:./mt",
    "ajv": "^8.3.0",
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "iconv-lite": "^0.4.24",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "mocha": "^9.0.2",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
