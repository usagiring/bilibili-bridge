{
  "name": "@tokine/bilibili-bridge",
  "version": "0.2.24",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:asr && npm run build:mt && npm run build:self",
    "build:self": "rm -rf dist && tsc && npm run copyproto",
    "copyproto": "cp ./lib/service/protobuf/*.proto ./dist/lib/service/protobuf",
    "start": "node ./dist/index",
    "start:test": "node ./bin/run-test.js",
    "dev": "nodemon --watch 'lib/**' --ext 'ts,json' --exec 'ts-node index.ts test'",
    "dev:win": "nodemon --watch \"lib/**\" --ext \"ts,json\" --exec \"ts-node index.ts test\"",
    "test": "mocha",
    "lint": "eslint lib/**/*",
    "release": "npm run build && pnpm publish",
    "build:asr": "cd packages/asr && npm run build",
    "build:mt": "cd packages/mt && npm run build"
  },
  "author": "其妙",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^3.4.3",
    "@koa/router": "^13.1.0",
    "@koa/send": "github:usagiring/send#master",
    "@node-rs/jieba": "^1.10.4",
    "@tokine/asr": "workspace:^",
    "@tokine/bilibili-recorder": "0.1.0",
    "@tokine/mt": "workspace:^",
    "ajv": "^8.17.1",
    "axios": "^1.7.9",
    "brotli": "^1.3.3",
    "cookie": "^0.6.0",
    "iconv-lite": "^0.4.24",
    "jsonwebtoken": "^8.5.1",
    "koa": "github:usagiring/koa#master",
    "koa-bodyparser": "^4.4.1",
    "koa-compose": "^4.1.0",
    "koa-logger": "^3.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nedb": "^1.8.0",
    "protobufjs": "^7.4.0",
    "say": "github:usagiring/say.js",
    "ws": "^7.5.10"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@types/nedb": "^1.8.16",
    "@types/node": "^22.13.1",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.22",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5"
  }
}
